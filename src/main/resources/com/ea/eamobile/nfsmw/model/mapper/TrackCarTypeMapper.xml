<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ea.eamobile.nfsmw.model.mapper.TrackCarTypeMapper">

	<resultMap id="trackCarTypeMap" type="com.ea.eamobile.nfsmw.model.TrackCarType">
		<result property="trackId" column="track_id" />
		<result property="carId" column="car_id" />
	</resultMap>

	<select id="getTrackCarTypes" resultMap="trackCarTypeMap">
		SELECT * from
		track_car_type;
	</select>


</mapper>
