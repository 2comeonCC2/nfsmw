<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ea.eamobile.nfsmw.model.mapper.FinishRatioMapper">

	<resultMap id="map" type="com.ea.eamobile.nfsmw.model.FinishRatio">
		<id property="id" column="id" />
		<result property="modeType" column="mode_type" />
		<result property="rank" column="rank" />
		<result property="value" column="value" />
	</resultMap>

	<select id="getFinishRatios" resultMap="map">
		SELECT * from finish_ratio
	</select>
</mapper>
